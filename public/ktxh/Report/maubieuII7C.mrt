              "ClientRectangle": "1.6,12.2,17.4,0.6",
              "Interaction": {
                "Ident": "StiInteraction"
              },
              "Text": {
                "Value": "o\tThu nhập bình quân đầu người (ước)...............................triệu đồng;"
